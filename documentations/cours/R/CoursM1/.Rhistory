NrNARow <- function(x){
sum(is.na(x));
}
manyNAs <- function(x, t){
tab <- apply(x, 1, NrNARow);
nb_val <- length(x);
in_p100_accept <- t * nb_val;
x[unname(tab) < in_p100_accept,];
}
centerImputation <- function(x){
for(i in 1:length(x)){
pos <- which(is.na(x[,i]) );
if(is.numeric(x[,i])){
x[pos,i]<-median(x[,i], na.rm=T);
}
if(is.factor(x[,i])){
x[pos,i]<-Mode(x[,i]);
}
}
(x);
}
