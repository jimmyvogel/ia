P1:
season1 <- prop.table(table(algae[algae[,12]=='feqLow', 1]));
season2 <- prop.table(table(algae[algae[,12]=='feqMedium', 1]));
season3 <- prop.table(table(algae[algae[,12]=='feqHigh', 1]));

size1 <- prop.table(table(algae[algae[,12]=='feqLow', 2]));
size2 <- prop.table(table(algae[algae[,12]=='feqMedium', 2]));
size3 <- prop.table(table(algae[algae[,12]=='feqHigh', 2]));

speed1 <- prop.table(table(algae[algae[,12]=='feqLow', 3]));
speed2 <- prop.table(table(algae[algae[,12]=='feqMedium', 3]));
speed3 <- prop.table(table(algae[algae[,12]=='feqHigh', 3]));

a1dens <- prop.table(table(algae[,12]))

p1 <- unname(season1[1])*unname(size1[1])*unname(speed1[3])*dlowmxph*dlowmno2*unname(a1dens[2])
p2 <- unname(season2[1])*unname(size2[1])*unname(speed2[3])*dmediummxph*dmediummno2*unname(a1dens[3])
p3 <- unname(season3[1])*unname(size3[1])*unname(speed3[3])*dhighmxph*dhighmno2*unname(a1dens[1]) 
