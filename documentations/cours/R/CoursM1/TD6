NrNARow <- function(x){
	sum(is.na(x));
}

manyNAs <- function(x, t){
	tab <- apply(x, 1, NrNARow);
	nb_val <- length(x);
	in_p100_accept <- t * nb_val;
	x[unname(tab) < in_p100_accept,];
}

centerImputation <- function(x){
	for(i in 1:length(x)){
		pos <- which(is.na(x[,i]) );
		if(is.numeric(x[,i])){
			x[pos,i]<-median(x[,i], na.rm=T);
		}
		if(is.factor(x[,i])){
			x[pos,i]<-Mode(x[,i]);
		}
	}
	(x);
}

algae_temp <- read.table('algaeClassification.txt', header=T)
algae_temp <- manyNAs(algae_temp, 0.2)
algae <- centerImputation(algae_temp)

Task 3:
pab1<-algae[algae[,1]=='winter' & algae[,12]=='feqLow',]
pb1<-algae[algae[,12]=='feqLow',]
p1<-length(pab1[,1])/length(pb1[,1])

pab2<-algae[algae[,2]=='medium' & algae[,12]=='feqHigh',]
pb2<-algae[algae[,12]=='feqHigh',]
p2<-length(pab2[,1])/length(pb2[,1])

pab3<-algae[algae[,3]=='low' & algae[,12]=='feqMedium',]
pb3<-algae[algae[,12]=='feqMedium',]
p3<-length(pab3[,1])/length(pb3[,1])


