me<-mean(v)
sd<-sd(v)
dmediummxph<-dnorm(8.25,me,sd)
v<-algae[algae[,12]=='feqHigh',4]
me<-mean(v)
sd<-sd(v)
dhighmxph<-dnorm(8.25,me,sd)
v<-algae[algae[,12]=='feqLow',4]
